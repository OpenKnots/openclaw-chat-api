# Upstash Vector
UPSTASH_VECTOR_REST_TOKEN=your_upstash_vector_rest_token
UPSTASH_VECTOR_REST_URL=your_upstash_vector_rest_url

# Upstash Redis (for rate limiting)
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Default chat model (gpt-5-nano, gpt-4.1-nano, gpt-4o-mini, gpt-5-mini, gpt-5.2)
DEFAULT_CHAT_MODEL=gpt-4o-mini

# GitHub Webhook (for docs auto-update)
# Generate with: openssl rand -hex 32
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret

# CORS - comma-separated list of allowed origins (leave empty to allow all)
ALLOWED_ORIGINS=https://yourdomain.com,https://docs.yourdomain.com
