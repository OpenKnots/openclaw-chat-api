{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/chat",
      "dest": "/api/chat"
    },
    {
      "src": "/(.*)",
      "dest": "/api/health"
    }
  ]
}
